@article{ WOS:A1997WN25000002,
Author = {Renaud, F},
Title = {Financial cost-benefit analysis of soil conservation practices in
   northern Thailand},
Journal = {MOUNTAIN RESEARCH AND DEVELOPMENT},
Year = {1997},
Volume = {17},
Number = {1},
Pages = {11-18},
Month = {FEB},
Abstract = {A financial cost-benefit analysis has been carried out on differentsoil
   conservation practices that have been studied on experimental plots in
   northern Thailand since 1989. Only one system, intercropping coffee with
   rice, has given a positive net present value (NPV) when a 26\% discount
   rate was used, reflecting the annual interest charged on loans within
   villages. A sensitivity analysis shows that another system, hillside
   ditches with coffee trees, and rice as a main crop, can also give a
   positive NPV, but only when a 0\% discount rate is used. It is argued
   that, in the context of agricultural sustainability, parameters other
   than the NPV should also be considered when selecting soil conservation
   practices for extension programs. When all the practices are compared,
   using criteria such as productivity, production risk, environmental
   protection, economic viability, and social acceptability, the
   intercropping system compares favorably with the others. These results
   are in accordance with field observations which showed that
   intercropping perennial crops with annual crops is, for the moment, one
   of the few adaptations that farmers in the study area are willing to
   make to their land and crop management systems.},
