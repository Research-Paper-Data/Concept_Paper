@article{ WOS:000404997600013,
Author = {Selinske, Matthew J. and Cooke, Benjamin and Torabi, Nooshin and Hardy,
   Mathew J. and Knight, Andrew T. and Bekessy, Sarah A.},
Title = {Locating financial incentives among diverse motivations for long-term
   private land conservation},
