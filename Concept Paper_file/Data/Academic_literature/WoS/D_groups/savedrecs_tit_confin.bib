@article{ WOS:000533069100003,
Author = {Abbasizade, F. and Abbaspour, M. and Soltanieh, M. and Kani, A.},
Title = {An innovative executive and financial mechanism for energy conservation
   in new and existing buildings in Iran},
Journal = {INTERNATIONAL JOURNAL OF ENVIRONMENTAL SCIENCE AND TECHNOLOGY},
Year = {2020},
Volume = {17},
Number = {10},
Pages = {4217-4232},
Month = {OCT 2},
Abstract = {One of the most effective measures for building energy conservation is
   the implementation of energy codes. The building energy codes
   introduction in Iran started two decades after the first oil crisis in
   the early 1970s by establishing the building energy conservation
   regulation called Issue 19. Nowadays, due to some financial and
   executive barriers, the implementation of Issue 19 is faced with some
   difficulties. The most critical obstacle is the low price of energy
   carriers, which lead to high subsidies of energy and the reluctance of
   the building owners to implement the regulation. The main goal of this
   paper is the introduction of an innovative financial and executive
   mechanism for implementation of Issue 19 in both new and existing
   buildings to achieve sustainability and resiliency. The cost-benefit
   analysis approach, along with SWOT technique and a comprehensive review
   of global experiences, was applied to introduce proper financial and
   executive mechanisms to determine the economic and social aspects of
   energy conservation programs for implementation of energy saving in
   buildings. The presented mechanism indicates the role of all different
   related parties, such as the government, banking system, energy-saving
   companies, building owners, and municipality, to achieve this goal. The
   results indicated that with a cost of \$10 million, the benefits of
   implementation of the introduced mechanism are over 5.78 billion
   dollars.},
